<document>
	<bank>
		<customer name="@customer_name@">
			<merchant cert_id="@cert_id@" name="@merchant_name@">
				<order order_id="@order_id@" amount="@amount@" currency="@currency@">
					<department merchant_id="@merchant_id@" amount="@amount@" abonent_id="@abonent_id@" terminal="@terminal@" phone="@phone@"/>
				</order>
			</merchant>
			<merchant_sign type="@althoritm@">@merchant_sign@</merchant_sign>
		</customer>
		<customer_sign type="@customer_sign_type@">@customer_sign@</customer_sign>

		<results timestamp="@timestamp@">
			<payment merchant_id="@merchant_id@" amount="@amount@"
				reference="@reference@" approval_code="@approval_code@" response_code="@response_code@" />
		</results>
	</bank>
	<bank_sign cert_id="@cert_id@" type="@bank_sign_type@">@bank_sign@</bank_sign>
</document>
